s.observer.disconnect();\n        this.observer = null;\n      }\n      if (this.$amapComponent) {\n        if (this.$amapComponent.getIsOpen()) {\n          this.$amapComponent.close();\n        }\n        this.$amapComponent = null;\n        this.$parentComponent = null;\n      }\n    },\n    __position(position) {\n      if (this.visible) {\n        this.$amapComponent.open(this.$parentComponent, position);\n      } else {\n        this.$amapComponent.setPosition(position);\n      }\n    },\n    __visible(flag) {\n      const position = this.$amapComponent.getPosition();\n      if (position) {\n        flag === false ? this.$amapComponent.close() : this.$amapComponent.open(this.$parentComponent, [position.lng, position.lat]);\n      }\n    }\n  }\n});\nexport { script as default };","map":{"version":3,"mappings":";;AAWA,aAAeA,eAAgB;EAC7BC,IAAM;EACNC,QAAQ,CAACC;EACTC,KAAO;IACLC,QAAU;MACRC,IAAM;MACNC,OAAS;IAAA;IAEXC,QAAU;MACRF,IAAM;MACNC,OAAS;IAAA;IAEXE,KAAO;MACLH,IAAM;IAAA;IAERI,iBAAmB;MACjBJ,IAAM;MACNC,OAAS;IAAA;IAEXI,OAAS;MACPL,MAAM,CAACM,MAAQ;IAAA;IAEjBC,IAAM;MACJP,IAAM;IAAA;IAERQ,MAAQ;MACNR,IAAM;IAAA;IAERS,MAAQ;MACNT,IAAM;IAAA;IAERU,QAAU;MACRV,IAAM;IAAA;EAAA;EAGVW,OAAO,CAAC;EACRC,IAAO;IACE;MACLC,UAAY;IAAA;EAAA;EAIhBC,SAAY;IACV,IAAG,KAAKC,cAAe;MACrB,KAAKA,cAAe;IAAA;EAAA;EAGxBC,OAAU;IACR,IAAG,CAAC,KAAKX,OAAW,SAAKU,cAAe;MACjC,oBAAeE,UAAW,MAAKC,KAAM;IAAA;EAAA;EAG9CC,OAAS;IACPC,gBAAgBC,OAAS;MACnB,KAACA,QAAQhB,OAAS;QACpBgB,QAAQhB,UAAU,IAAK;MAAA;MAEpB,sBAAiB,IAAIiB,KAAKC,UAAW;MACrC,oBAAeC,EAAG,UAAS,MAAM;QACpC,KAAKC,MAAM,gBAAkB;MAAA;MAE3B,SAAKC,YAAY,KAAO;QAC1B,KAAKX,cAAe,MAAK,IAAK,mBAAkB,IAAK;MAAA;MAEvD,IAAG,CAAC,KAAKV,OAAW,SAAKU,cAAe;QAEtC,MAAMY,SAAS;UAAEC,YAAY,IAAM;UAAAC,WAAW;UAAMC,OAAS;QAAA;QAEvD,iBAAW,IAAIC,iBAAiB,MAAM;UACrC,oBAAed,WAAW,IAAK;QAAA;QAE7Be,iBAAQ,IAAK,OAAMC,IAAc;QAC1C,KAAKD,QAAW;MAAA;IAAA;IAGpBE,cAAgB;MACd,OAAQ,KAAKhB,KAAM;IAAA;IAErBiB,gBAAmB;MACjB,IAAG,KAAKH,QAAS;QACf,KAAKA,QAAS;QACd,KAAKA,QAAW;MAAA;MAElB,IAAG,KAAKjB,cAAe;QAClB,SAAKA,eAAeqB,SAAY;UACj